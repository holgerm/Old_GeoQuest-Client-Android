<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.bonn.mobilegaming.geoquest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.NFC" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application
        android:name="GeoQuestApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <uses-library android:name="com.google.android.maps" />

        <activity
            android:name="com.qeevee.gq.start.LandingScreen"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.qeevee.gq.start.InvisibleStart"
            android:label="@string/app_name"
            android:noHistory="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="geoquest" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.qeevee.gq.start.GamesInCloud"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.qeevee.gq.start.LocalGames"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.qeevee.gq.history.HistoryActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.qeevee.ui.FullScreenImage"
            android:label="@string/app_name"
            android:screenOrientation="sensor" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.StartAndExitScreen"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.MapGoogle"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.MapOSM"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.NPCTalk"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.WebPage"
            android:label="@string/app_name"
            android:screenOrientation="sensor" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.WebTech"
            android:label="@string/app_name"
            android:screenOrientation="sensor" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.QRTagReading"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.NFCTag"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.ImageCapture"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.AudioRecord"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.VideoRecord"
            android:label="@string/quest_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.PhotoPuzzle"
            android:label="@string/quest_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.VideoPlay"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.MultipleChoiceQuestion"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.TextQuestion"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.Preferences"
            android:label="@string/preferencesTitle"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="edu.bonn.mobilegaming.geoquest.mission.ExternalMission"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
    </application>

</manifest>