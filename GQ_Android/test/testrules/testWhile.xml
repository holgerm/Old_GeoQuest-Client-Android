<?xml version="1.0" encoding="UTF-8"?>

<test>
	<rule>
		<action type="Loop">
			<condition>
				<Leq>
					<var>score</var>
					<num>10</num>
				</Leq>
			</condition>
			<then>
				<action type="AddToScore" value="2" />
				<action type="IncrementVariable" var="counter"/>
			</then>
		</action>
	</rule>
	<rule>
		<action type="Loop">
			<condition>
				<eq>
					<num>1</num>
					<num>10</num>
				</eq>
			</condition>
			<then>
				<action type="SetVariable" var="staysEmpty" value="10.0"/>
			</then>
		</action>
	</rule>
</test>
